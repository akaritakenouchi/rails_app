<h3>Tweet一覧</h3>
<div class="link"><%= link_to "新規投稿へ", new_tweet_path %></div>


<%= link_to "あいうえお", new_tweet_path %>

<div class="tweets-container">
  <% @tweets.each do |t| %>
    <div class="post">
      <%= t.body %>
      <%= t.endday %>

      <%= link_to "詳細へ", tweet_path(t.id) %>
      <%= link_to "編集する", edit_tweet_path(t.id) %>
      <%= button_to "削除する", tweet_path(t.id), method: :delete %>

    </div>
  <% end %>
</div>
