<h1>GeekTwitter</h1>
<h3>新規Tweet</h3>
<%= form_for @tweet do |f| %>
<div class="field">
    <%= f.label :body %>
    <%= f.text_field :body, :size => 140 %>
    <%= f.label :endday %>
    <%= f.text_field :endday, :size => 140 %>

</div>
<%= f.submit "Tweetする" %>
<% end %>
<%= link_to "Tweet一覧に戻る", tweets_path %>